{
  "expo": {
    "name": "YARD",
    "slug": "yard-workout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yard",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yardfitness.app",
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "YARD needs camera access to take a profile photo for your Rec Yard community profile. For example, you can snap a selfie to show other members who you are.",
        "NSPhotoLibraryUsageDescription": "YARD needs access to your photo library to select a profile photo for your Rec Yard community profile. For example, you can choose an existing photo to represent yourself in the community."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0b0b0b",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yardfitness.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#0b0b0b",
          "dark": {
            "backgroundColor": "#0b0b0b"
          }
        }
      ],
      "expo-web-browser",
      "expo-image-picker",
      "expo-secure-store"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "f01b967b-9012-4ee7-8f44-e6d6baac3419"
      }
    }
  }
}
