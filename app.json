{
  "expo": {
    "name": "YARD",
    "slug": "yard-workout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "yard",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yardfitness.app",
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": []
      },
      "infoPlist": {
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "YARD requires camera access to allow you to take a profile photo. This photo is displayed on the global leaderboard to verify your identity to other members. For example, you can take a selfie to use as your avatar.",
        "NSPhotoLibraryUsageDescription": "YARD requires photo library access to allow you to upload a profile photo. This photo is displayed on the global leaderboard. For example, you can select an existing photo from your library to use as your avatar."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0b0b0b",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.yardfitness.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#0b0b0b",
          "dark": {
            "backgroundColor": "#0b0b0b"
          }
        }
      ],
      "expo-web-browser",
      "expo-image-picker",
      "expo-secure-store"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "eas": {
        "projectId": "f01b967b-9012-4ee7-8f44-e6d6baac3419"
      }
    }
  }
}